        function ClearClasses() {
            var children = document.getElementById('TabButtons').children;

            for (var i = 0; i < children.length; i++) {
                children[i].className = 'BillTabs';
            }
        }

        function WriteToIframe() {
            var iframe = window.frames['BFrame'];
            var html = document.getElementById('hfIframeText').value;

            iframe.document.open();
            iframe.document.write(html);
            iframe.document.close();
        }

